{
	"description": "A responsive image workflow for optimizing and resizing your images",
	"homepage": "https://github.com/jamesjohnmcguire/PhpImageOptimizer/",
	"license": "MIT",
	"name": "digitalzenworks/php-image-optimizer",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"type": "library",
	"authors": [
		{
			"name": "James John McGuire",
			"email": "jamesjohnmcguire@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"DigitalZenWorks\\": "SourceCode/"
		}
	},
	"keywords": [
		"responsive images",
		"respimg",
		"responsive web design",
		"rwd",
		"imagemagick",
		"imagick",
		"resize"
	],
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/jamesjohnmcguire/PhpImageOptimizer/"
		}
	],
	"require": {
		"php": ">=7.1.0",
		"ext-imagick": "*",
		"gumlet/php-image-resize": "^2.0"
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "*",
		"php-parallel-lint/php-console-highlighter": "^1.0",
		"php-parallel-lint/php-parallel-lint": "^1.2.0",
		"phpcompatibility/php-compatibility": "*",
		"phpcsstandards/phpcsdevtools": "^1.0",
		"phpcsstandards/phpcsextra": "^1.2.0",
		"phpstan/extension-installer": "^1.4",
		"phpstan/phpstan": "^2.0",
		"phpstan/phpstan-deprecation-rules": "^2.0",
		"phpunit/phpunit": "^10.0",
		"roave/security-advisories": "dev-master",
		"squizlabs/php_codesniffer": "*"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"phpstan/extension-installer": true
		}
	}
}
